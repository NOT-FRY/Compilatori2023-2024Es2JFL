public class Main {
    public static void main(String[] args) {

        System.out.println("Benvenuto nel mio programma Java!");


        int numero = 42;


        if (numero > 0) {
            System.out.println("Il numero è positivo.");
        } else if (numero < 0) {
            System.out.println("Il numero è negativo.");
        } else {
            System.out.println("Il numero è zero.");
        }


        for (int i = 1; i <= 5; i++) {
            System.out.println("Iterazione " + i);
        }

        System.out.println("Grazie per aver usato il mio programma!");
    }
}